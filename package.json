{
  "name": "@chameleoncloud/jupyterlab_zenodo",
  "version": "0.3.0",
  "description": "JupyterLab extension for uploading to Zenodo",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "zenodo"
  ],
  "homepage": "https://github.com/ChameleonCloud/jupyterlab-zenodo",
  "bugs": {
    "url": "https://github.com/ChameleonCloud/jupyterlab-zenodo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChameleonCloud/jupyterlab-zenodo.git"
  },
  "license": "MIT",
  "author": "Maxine King <maxineking@uchicago.edu>",
  "files": [
    "lib/*/*d.ts",
    "lib/*/*.js",
    "lib/*.d.ts",
    "lib/*.js",
    "style/**/*.*",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "test": "make tests",
    "watch": "tsc -w"
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@jupyterlab/application": "~1.2.0",
    "@jupyterlab/apputils": "*",
    "@jupyterlab/coreutils": "*",
    "@jupyterlab/filebrowser": "*",
    "@jupyterlab/fileeditor": "*",
    "@jupyterlab/mainmenu": "*",
    "@jupyterlab/services": "*",
    "@jupyterlab/ui-components": "*",
    "@phosphor/coreutils": "*",
    "@phosphor/widgets": "*"
  },
  "devDependencies": {
    "@types/react": "~16.8.18",
    "@types/react-dom": "~16.8.4",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.18.2",
    "rimraf": "~2.6.2",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "~3.7.2"
  },
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyterlab_zenodo"
        }
      }
    },
    "schemaDir": "schema"
  }
}
