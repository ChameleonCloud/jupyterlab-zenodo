{
  "jupyter.lab.setting-icon-class": "jp-TextEditorIcon",
  "jupyter.lab.setting-icon-label": "Zenodo",
  "title": "Zenodo Extension",
  "description": "Zenodo extension settings.",
  "properties": {
    "createLabel": {
      "type": "string",
      "title": "Title for create command",
      "default": "Create Zenodo deposition"
    },
    "editLabel": {
      "type": "string",
      "title": "Title for edit command",
      "default": "Edit Zenodo metadata"
    },
    "baseUrl": {
      "type": "string",
      "title": "Base URL for rendered Zenodo deposition URLs",
      "default": "https://zenodo.org"
    },
    "externalEditUrl": {
      "type": "string",
      "title": "External URL for editing depositions",
      "default": ""
    }
  },
  "additionalProperties": false,
  "type": "object"
}
